
@model IstanbulNsApp.Resources.DTO.OnlineTurnDTO

<form method="p">

    <div class="form-group">
        <label for="phone">Telefon Nömrəsi</label>
        <input type="text" name="phone" id="phone" placeholder="Telefon Nömrəsi">
    </div>
    <div class="form-group">
        <label for="mövzu">Mövzu</label>
        <input type="text" name="subject" id="subject" placeholder="Mövzu">
    </div>
    <div class="form-group">
        <label for="message">Mesajınız</label>
        <textarea id="message" name="message" rows="5"></textarea>
    </div>

    <div class="btn-div">
        <button type="submit" name="submit">Göndər</button>
    </div>
</form>